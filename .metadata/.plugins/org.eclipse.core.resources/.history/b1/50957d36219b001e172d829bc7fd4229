package ece.btech2;

public class Program5 {

	public static void main(String[] args) {
		
		System.out.println("Hi Aparna How are you?");
		System.out.println("Hi Uday How are you?");
		System.out.println("Hi Uma How are you?");
		System.out.println("Hi Murthy How are you?");

	}
	public static void namePrint(String name) {
		System.out.println("Hi "+name+" How are you?");
	}
}
